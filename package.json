{
  "name": "infinitysoul-platform",
  "version": "2.0.0",
  "description": "InfinitySoul WCAG Compliance Intelligence Platform - Enterprise-grade accessibility scanning and compliance management",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean && rm -rf node_modules .turbo",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "db:generate": "turbo run db:generate",
    "db:push": "turbo run db:push",
    "db:migrate": "turbo run db:migrate",
    "db:studio": "cd packages/database && npx prisma studio",
    "type-check": "turbo run type-check",
    "vercel-build": "turbo run build --filter=web"
  },
  "keywords": [
    "wcag",
    "accessibility",
    "a11y",
    "compliance",
    "infinitysoul",
    "ada",
    "section-508",
    "enterprise",
    "saas"
  ],
  "author": "InfinitySoul LLC",
  "license": "PROPRIETARY",
  "repository": {
    "type": "git",
    "url": "https://github.com/aaj441/WCAGAI.git"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "npm@10.2.0",
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "turbo": "^1.11.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@prisma/client": "^5.8.0",
    "@vercel/analytics": "^1.1.1",
    "@vercel/speed-insights": "^1.0.2"
  }
}